<?xml version="1.0" encoding="utf-8"?>
<schema name = "name">
	<facts>
        <fact name="HP_Player_Full">
            <attributes>
                <attrib target = "attribute//HP" filter = "9" />
                <attrib target = "label" filter = "Player" />
            </attributes>
			<info value="attribute//HP" />
		</fact>
        <fact name="Round">
            <attributes>
                <attrib target = "label" filter = "Round" />
            </attributes>
			<info value="label" />
		</fact>
        <fact name="Round">
            <attributes>
                <attrib target = "label" filter = "Round" />
            </attributes>
			<info value="label" />
		</fact>
        <fact name="BeingHit_Player">
             <attributes>
                <attrib target="label" filter="Being Hit(Player)" />
            </attributes>
			<info value="attribute//Last" />
		</fact>
        <fact name="BeingHit_Enemy">
             <attributes>
                <attrib target="label" filter="Being Hit(Enemy)" />
            </attributes>
			<info value="attribute//InstanceID" />
		</fact>
        <fact name="Attacking_Enemy">
             <attributes>
                <attrib target="label" filter="Attacking (Enemy)2" />
            </attributes>
			<info value="attribute//ObjectName" />
		</fact>
     </facts>
     <rules>
            <rule name="playerHit">
               <inputs>
                    <input name="X"/>
                    <input name="Y"/>
               </inputs>
               <facts name="facts_1">
                    <fact name="f_1.1">
                        <input name="X"/>
                        <input name="Y"/>
                    </fact>
                    <fact name="f_1.2">
                        <input name="A"/>
                        <input name="B"/>
                        <input name="C"/>
                        <input name="D"/>
                    </fact>
               </facts>
          </rule>
            <rule name="fact_test">
               <inputs>
                    <input name="X_test"/>
                    <input name="Y_test"/>
               </inputs>
               <facts name="facts_1_test">
                    <fact name="f_1.1_test">
                        <input name="X_test"/>
                        <input name="Y_test"/>
                    </fact>
                    <fact name="f_1.2_test">
                        <input name="A_test"/>
                    </fact>
               </facts>
          </rule>
            <rule name="fact_1">
               <inputs>
                    <input name="X_1"/>
                    <input name="Y_1"/>
               </inputs>
               <facts name="facts_1_test">
                    <fact name="f_1.1_test">
                        <input name="X_1"/>
                        <input name="Y_1"/>
                    </fact>
               </facts>
          </rule>
            <rule name="Attacking">
               <inputs>
                    <input name="shooter"/>
                    <input name="target"/>
               </inputs>
               <facts name="facts_1">
                    <fact name="Attacking_Enemy">
                        <input name="shooter"/>
                        <input name="target"/>
                    </fact>
                    <fact name="BeingHit_Enemy">
                        <input name="target"/>
                        <input name="shooter"/>
                    </fact>
               </facts>
          </rule>
            <rule name="Counter">
               <inputs>
                    <input name="shooter"/>
               </inputs>
               <facts name="facts_1">
                    <fact name="BeingHit_Player">
                        <input name="_x"/>
                        <input name="shooter"/>
                    </fact>
                    <fact name="BeingHit_Enemy">
                        <input name="_y"/>
                        <input name="shooter"/>
                    </fact>
               </facts>
          </rule>
   </rules>
</schema>