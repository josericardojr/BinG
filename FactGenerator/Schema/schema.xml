<?xml version="1.0" encoding="utf-8"?>
<schema name = "name">
	<facts>
        <fact name="HP">
			<attrib target="label" filter="Player" value="attribute//HP" />
		</fact>
        <fact name="ObjectPosition_X">
			<attrib target="attribute//ObjectPosition_X" filter="4.665031" value="label" />
		</fact>
        <fact name="attribute">
			<attrib target="attribute//LocalTime" filter="19.08652" value="label" />
		</fact>
		<fact name="f_player">
		   <attrib target="label" filter="Player" value="label"/>
		</fact> 
		<fact name="f_round">
			<attrib target="label" filter="Round" value="label"/>
		</fact>
		<fact name="type"> 
			<attrib target="type" filter="Agent" value="label" />
		</fact>
     </facts>
     <rules>
            <rule name="playerHit">
               <inputs>
                    <input name="X"/>
                    <input name="Y"/>
               </inputs>
               <facts name="facts_1">
                    <fact name="f_1.1">
                        <input name="X"/>
                        <input name="Y"/>
                    </fact>
                    <fact name="f_1.2">
                        <input name="A"/>
                        <input name="B"/>
                        <input name="C"/>
                        <input name="D"/>
                    </fact>
               </facts>
          </rule>
            <rule name="fact_test">
               <inputs>
                    <input name="X_test"/>
                    <input name="Y_test"/>
               </inputs>
               <facts name="facts_1_test">
                    <fact name="f_1.1_test">
                        <input name="X_test"/>
                        <input name="Y_test"/>
                    </fact>
                    <fact name="f_1.2_test">
                        <input name="A_test"/>
                    </fact>
               </facts>
          </rule>
            <rule name="fact_1">
               <inputs>
                    <input name="X_1"/>
                    <input name="Y_1"/>
               </inputs>
               <facts name="facts_1_test">
                    <fact name="f_1.1_test">
                        <input name="X_1"/>
                        <input name="Y_1"/>
                    </fact>
               </facts>
          </rule>
   </rules>
</schema>