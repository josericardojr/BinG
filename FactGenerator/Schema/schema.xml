<?xml version="1.0" encoding="utf-8"?>
<schema name = "name">
	<facts>
		<fact name="f_player">
		   <attrib name="label" filter="Player" value="player_label"/>
		</fact> 
		<fact name="f_round">
			<attrib name="label" filter="Round" value="round_label"/>
		</fact>  
		<fact name="vertexType"> 
			<attrib name="type"/>
		</fact> 
     </facts>
     <rules>
          <rule name="playerHit">
               <inputs>
                    <input name="X"/>
                    <input name="Y"/>
               </inputs>
               <facts>
                    <fact name="f_player">
                         <input name="X"/>
                    </fact>
                    <fact name="f_round">
                    <input name="Y"/>
                    </fact>
               </facts>
          </rule>
   </rules>
</schema>