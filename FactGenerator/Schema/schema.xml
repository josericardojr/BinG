<?xml version="1.0" encoding="utf-8"?>
<schema name = "name">
     <facts>
          <fact name="f_player">
               <attrib name="label" filter="Player" value="player_label"/>
          </fact> 
          <fact name="f_enemy">
               <attrib name="label" filter="Round" value="round_label"/>
          </fact> 
     </facts>
     <rules>
          <rule name="playerHit">
               <inputs>
                    <input name="X"/>
                    <input name="Y"/>
               </inputs>
               <facts>
                    <fact name="f_player">
                         <input name="X"/>
                    </fact>
                    <fact name="f_enemy">
                    <input name="Y"/>
                    </fact>
               </facts>
          </rule>
   </rules>
</schema>